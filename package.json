{
  "name": "@azurepipelines/kubernetesui-devopsextension",
  "version": "0.0.6",
  "description": "Azure DevOps Kubernetes Summary",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/azPipeline-KubernetesUI-devopsExtension.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode development",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/*.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/*.json"
  },
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "main": "dist/WebUI/Components/KubeSummary.js",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@azurepipelines/azdevops-kube-summary": "^1.0.8",
    "@kubernetes/client-node": "^0.7.2",
    "azure-devops-extension-api": "^1.143.0",
    "azure-devops-extension-sdk": "~2.0.3",
    "react": "~16.3.2",
    "react-dom": "~16.3.2"
  },
  "devDependencies": {
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.9",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "file-loader": "~2.0.0",
    "node-sass": "~4.9.4",
    "recursive-copy": "~2.0.7",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "uglify-es": "~3.1.3",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/azPipeline-KubernetesUI-devopsExtension/issues"
  },
  "homepage": "https://github.com/Microsoft/azPipeline-KubernetesUI-devopsExtension#readme",
  "author": "Azure Pipelines"
}
