{
    "compilerOptions": {
        "charset": "utf8",
        "declaration": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "lib": [
            "es5",
            "es6",
            "dom",
            "es2015.promise"
          ],
        "module": "amd",
        "target": "es5",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noImplicitThis": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "rootDir": "src/",
        "baseUrl": "./src",
        "strict": true,
        "paths": {
            "@azurepipelines/kubernetesui-devopsextension/dist/*": [
                "./*"
            ]
        }
    }
}